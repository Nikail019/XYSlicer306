// Motor speed control/enable
int E1 = 5;
int M1 = 4;
int E2 = 6;
int M2 = 7;

void setup()
{
    pinMode(M1, OUTPUT);
    pinMode(M2, OUTPUT);
    // pinMode(A1, INPUT);
    // pinMode(B1, INPUT);
    // attachInterrupt(digitalPinToInterrupt(EncA1), printEncoder, CHANGE);
    // attachInterrupt(digitalPinToInterrupt(EncB1), printEncoder, CHANGE);
    //Serial.begin(9600); // On Mega there is also Serial1, Serial2 [ Serial1.begin(0)..., Serial1.println()]
    
}

void loop()
{
  int value;
  for(value = 0 ; value <= 255; value+=5)
  {
    digitalWrite(M1,HIGH);
    digitalWrite(M2, HIGH);
    analogWrite(E1, value);   //PWM Speed Control
    analogWrite(E2, value);   //PWM Speed Control
    delay(30);
  }
}

